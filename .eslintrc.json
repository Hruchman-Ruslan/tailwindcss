{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:tailwindcss/recommended"
  ],
  "plugins": [
    "prettier",
    "tailwindcss"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "no-unused-vars": [
      "error",
      { "args": "none" }
    ],
    "tailwindcss/classnames-order": "warn",
    "tailwindcss/no-custom-classname": "warn"
  }
}
